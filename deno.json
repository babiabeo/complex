{
    "name": "@babia/complex",
    "version": "1.0.0",
    "exports": {
        ".": "./mod.ts",
        "./math": "./src/math/mod.ts"
    },
    "fmt": {
        "indentWidth": 4
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@^0.225.3"
    },
    "exclude": [".vscode", "docs"],
    "tasks": {
        "check": "deno publish --dry-run",
        "lint": "deno fmt --check && deno lint",
        "test": "deno test",
        "ok": "deno task lint && deno task check && deno task test",
        "doc:view": "deno doc --html --name=\"@babia/complex\" ./mod.ts"
    }
}
